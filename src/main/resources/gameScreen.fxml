<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GameController">
    <children>
        <Pane fx:id="panee" maxHeight="720.0" maxWidth="720.0" prefHeight="720.0" prefWidth="720.0" style="-fx-background-color: #333333;" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="play" layoutX="598.0" layoutY="682.0" mnemonicParsing="false" onAction="#playGame" text="Play" />
                <Button fx:id="pause" layoutX="649.0" layoutY="682.0" mnemonicParsing="false" onAction="#pauseGame" text="Pause" />
                <Pane fx:id="playField" layoutX="31.0" layoutY="6.0" maxWidth="522.0" minWidth="522.0" onMouseClicked="#handleMouse" onMouseMoved="#mouseArrow" prefHeight="688.0" prefWidth="522.0" style="-fx-background-color: #FFFFFF;" />
                <Text fx:id="scoreText" fill="WHITE" layoutX="583.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score: 0" wrappingWidth="105.0">
                    <font>
                        <Font size="36.0" />
                    </font>
                </Text>
            </children>
        </Pane>
    </children>
</AnchorPane>
