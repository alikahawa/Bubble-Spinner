<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.PasswordField?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.StartController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="426.0" prefWidth="682.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="exit" layoutX="188.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleExitButtonAction" onMouseClicked="#handleExitButtonAction" prefHeight="40.0" prefWidth="230.0" text="Exit" />
                <TextField fx:id="username" layoutX="188.0" layoutY="133.0" prefHeight="26.0" prefWidth="230.0" promptText="Username" />
                <PasswordField fx:id="password" layoutX="188.0" layoutY="187.0" prefHeight="26.0" prefWidth="230.0" promptText="Password" />
                <Button fx:id="logIn" defaultButton="true" layoutX="188.0" layoutY="238.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" onMouseClicked="#handleLoginButtonAction" prefHeight="40.0" prefWidth="230.0" text="Log-In" />
            </children>
        </AnchorPane>
    </children>
</VBox>
